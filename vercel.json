{
  "crons": [
    {
      "path": "/api/cron/process-recordings-v3",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/cron/process-transcription-queue",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/cron/queue-bulk-recordings",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/jobs/batch",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/jobs/backfill",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/rollup",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/retention",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/convoso-delta",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/convoso-sync",
      "schedule": "*/5 * * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/retention/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/admin/backfill/route.ts": {
      "maxDuration": 300
    }
  }
}
